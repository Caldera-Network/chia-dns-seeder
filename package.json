  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caldera-network/dns-seeder/issues"
  },
  "homepage": "https://github.com/caldera-network/dns-seeder#readme",
  "devDependencies": {
    "@types/cloudflare": "^2.7.3",
    "@types/node": "^16.4.13",
    "@types/update-notifier": "^5.1.0",
    "prettier": "^2.1.2",
    "tsc-alias": "^1.3.8",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@caldera-network/chia-network-scanner": "^6.0.2",
    "axios": "^0.21.1",
    "cloudflare": "^2.7.0",
    "commander": "^8.1.0",
